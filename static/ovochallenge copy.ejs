    <style>
        @import url(//fonts.googleapis.com/css?family=Roboto+Slab:400);

        body {
            background: #40404000;
        }

        .cover {
            position: absolute;
            top: 0;
            left: 0;
            z-index: 2;
            width: 100%;
            height: 100%;
        }

        .cover .hi {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: #fff;
            font-family: "Roboto Slab", serif;
            font-size: 24px;
            line-height: 26px;
            text-align: center;
        }

        .cover .hi span {
            cursor: pointer;
            text-decoration: underline;
        }

        .cover .hi em {
            font-style: normal;
        }

        .cover .hi em.hidden {
            display: none;
        }

        .tv {
            position: absolute;
            top: 0;
            left: 0;
            z-index: 1;
            width: 100%;
            height: 100%;
            overflow: hidden;
        }

        .tv .screen {
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            z-index: 1;
            margin: auto;
            opacity: 0;
            transition: opacity 0.5s;
        }

        .tv .screen.active {
            opacity: 1;
        }
    </style>
</head>

